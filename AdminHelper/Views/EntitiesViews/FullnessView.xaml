<UserControl x:Class="AdminHelper.Views.EntitiesViews.FullnessView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataGrid ItemsSource="{Binding Entities}"
                  AutoGenerateColumns="False">
        <DataGrid.Columns>
            <DataGridTextColumn Binding="{Binding Id}" Header="Id" IsReadOnly="True"/>
            <DataGridTextColumn Binding="{Binding FullnessName}" Header="Наполненность"/>
            <DataGridTextColumn Binding="{Binding Allowance}" Header="Надбавка"/>
        </DataGrid.Columns>
    </DataGrid>
</UserControl>
